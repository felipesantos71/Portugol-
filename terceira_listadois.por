programa {
  //incio do programa
  funcao inicio() {
  //declarando variaveis
  inteiro idade
  //solicitando dados do usuário
  escreva("Digite sua idade: ")
  leia(idade)
  //estrutura condicional
  se (idade >=18 e idade<=65 ) {
      escreva("\nVocê é obrigado a votar!")
  } senao {
    escreva("\nVocê não é obrigado a votar!")
  }

  }
  //fim do programa
}
